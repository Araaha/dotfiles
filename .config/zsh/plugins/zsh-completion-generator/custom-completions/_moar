#compdef moar

# zsh completions for 'moar'
# automatically generated with http://github.com/RobSis/zsh-completion-generator
local arguments

arguments=(
  '-colors[highlighting palette size: 8, 16, 256, 16M, auto]'
  '-debug[print debug logs after exiting]'
  '-follow[follow piped input just like "tail -f"]'
  '-lang[file contents, used for highlighting. Mime type or file extension ("html"). Default is to guess by filename.]'
  '-mousemode[mouse mode: auto, select or scroll: https://github.com/walles/moar/blob/master/MOUSE.md]'
  '-no-clear-on-exit[retain screen contents when exiting moar]'
  '-no-linenumbers[hide line numbers on startup, press left arrow key to show]'
  '-no-reformat[no effect, kept for compatibility. See --reforamat (default true)]'
  '-no-statusbar[hide the status bar, toggle with =]'
  '-quit-if-one-screen[dont page if contents fits on one screen]'
  '-reformat[reformat some input files (JSON)]'
  '-render-unprintable[how unprintable characters are rendered: highlight or whitespace]'
  '-scroll-left-hint[shown when view can scroll left. One character with optional ANSI highlighting.]'
  '-scroll-right-hint[shown when view can scroll right. One character with optional ANSI highlighting.]'
  '-shift[horizontal scroll amount >=1, defaults to 16]'
  '-statusbar[status bar style: inverse, plain or bold]'
  '-style[highlighting style from https://xyproto.github.io/splash/docs/longer/all.html]'
  '-terminal-fg[use terminal foreground color rather than style foreground for plain text]'
  '-trace[print trace logs after exiting]'
  '-version[prints the moar version number]'
  '-wrap[wrap long lines]'
  '*:filename:_files'
)

_arguments -s $arguments
