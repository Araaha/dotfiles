#!/usr/bin/env xdg-open
[Desktop Entry]
Type=Application
TryExec=alacritty
Exec=alacritty -o 'window.startup_mode="Maximized"' -o 'window.opacity=1' -e tmux
Icon=Alacritty
Terminal=false
Categories=System;TerminalEmulator;

Name=Alacritty
GenericName=Terminal
Comment=A fast, cross-platform, OpenGL terminal emulator
StartupWMClass=Alacritty
Actions=New;

X-Desktop-File-Install-Version=0.26

[Desktop Action New]
Name=New Terminal
Exec=alacritty
